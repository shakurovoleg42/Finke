{
    "version": 2,
    "builds": [
      {
        "src": "src/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "src/js/script.js",
        "use": "@vercel/static"
      },
      {
        "src": "src/css/*.css",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  